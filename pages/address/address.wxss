.fabu {position: fixed; top: 0; left: 0; z-index: 99; width: 100%; height: 80rpx; background: #fff;
display: flex; align-items: center; justify-content: center; border-bottom: 1rpx solid #eee;}
.fabu image{
  width: 30rpx;
  height: 30rpx; margin-right:20rpx;
}
.fabu text{
  font-size: 30rpx; color: #ff3131;
}
.address{ padding-top: 80rpx; }

.addresslist{border-bottom: 15rpx solid #f5f5f5;}
.addresslist .top{ padding:0 30rpx; padding-top: 10rpx; display: flex; justify-content: space-between; line-height: 60rpx;}
.addresslist .xiangxi{ padding:10rpx 30rpx; padding-bottom: 20rpx; line-height: 36rpx; color: #666; border-bottom: 1rpx solid #eee;}
.addresslist .guanli{ padding:0 30rpx; display: flex; justify-content: space-between; line-height: 80rpx; color: #777;}
.addresslist .bianji{display: flex; align-items: center;}
.addresslist .bianji view{display: flex; align-items: center; margin-left: 30rpx;}
.addresslist .moren {display: flex; align-items: center; height: 80rpx;}
.addresslist .moren icon{ margin-right: 15rpx; margin-top:28rpx;}
.addresslist .bianji image{display: inline-block; margin-right: 8rpx; width: 30rpx; height: 30rpx;}
